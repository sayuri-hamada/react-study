    "@babel/preset-env": "7.18.10",
    "@babel/preset-typescript": "7.18.6",
    "@types/node": "18.7.6",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "autoprefixer": "10.4.8",
    "babel-loader": "8.2.5",
    "babel-plugin-transform-remove-console": "6.9.4",
    "browserslist": "4.21.3",
    "chalk": "4.1.2",
