{
  "name": "nextjs-starter",
  "version": "2.2.2",
  "main": "index.js",
  "repository": "git@github.com:ozizo/nextjs-starter.git",
  "license": "MIT",
  "browserslist": [
    "> 0.2%, not dead",
    "not IE 11",
    "Safari > 13, iOS > 13"
  ],
  "scripts": {
    "prepare": "rimraf .config && husky install .config/husky && husky add .config/husky/pre-commit \"yarn lint-staged\"",
    "dev": "node server",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true yarn start",
    "export": "yarn build && next export && next-sitemap --config sitemap.config.js",
    "start": "yarn build && next start -p 8888",
    "clean": "rimraf .next out",
    "lint": "yarn lint:script && yarn lint:style && yarn prettier",
    "lint:style": "stylelint --fix '**/*.scss'",
    "lint:script": "eslint --fix --ext .ts,.tsx --ignore-pattern .eslintignore .",
    "prettier": "prettier --write .**/*.{js,json}",
    "svgr": "yarn svgr:icon",
    "svgr:icon": "svgr --filename-case kebab src/assets/images/icons/**/*.svg --ext tsx --out-dir src/components/icons",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "generate": "sg wizard"
  },
  "devDependencies": {
